{
  "name": "@fluxomail/sdk",
  "version": "0.2.0",
  "description": "Fluxomail SDK for JS/TS: send emails, list/subscribe to events, get timelines.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "fluxomail": "bin/fluxomail.mjs"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --sourcemap --clean",
    "dev": "tsup src/index.ts --dts --format esm --sourcemap --watch",
    "pretest": "npm run build",
    "test": "node --test",
    "test:coverage": "c8 --reporter=text --reporter=lcov --exclude=bin/** --exclude=tests/** --exclude=**/src/events/stream.ts npm test",
    "commitlint": "commitlint --from=origin/main --to=HEAD --verbose",
    "setup:hooks": "git config core.hooksPath .husky",
    "prepare": "npm run build"
  },
  "keywords": [
    "email",
    "sdk",
    "sse",
    "events",
    "fluxomail"
  ],
  "author": "Fluxomail",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluxomail/fluxomail-sdk-js.git"
  },
  "homepage": "https://docs.fluxomail.com/api-reference/fluxomail/cli",
  "bugs": {
    "url": "https://github.com/fluxomail/fluxomail-sdk-js/issues"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "preferGlobal": true,
  "dependencies": {
    "eventsource": "4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "c8": "^9.1.0",
    "husky": "^9.1.6",
    "tsup": "^8.1.0",
    "typescript": "^5.6.3"
  }
}
